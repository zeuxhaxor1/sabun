get_lang(){global$ca;return$ca;}function
lang($u,$mf=null){if(is_string($u)){$jg=array_search($u,get_translations("en"));if($jg!==false)$u=$jg;}global$ca,$zi;$yi=($zi[$u]?$zi[$u]:$u);if(is_array($yi)){$jg=($mf==1?0:($ca=='cs'||$ca=='sk'?($mf&&$mf<5?1:2):($ca=='fr'?(!$mf?0:1):($ca=='pl'?($mf%10>1&&$mf%10<5&&$mf/10%10!=1?1:2):($ca=='sl'?($mf%100==1?0:($mf%100==2?1:($mf%100==3||$mf%100==4?2:3))):($ca=='lt'?($mf%10==1&&$mf%100!=11?0:($mf%10>1&&$mf/10%10!=1?1:2)):($ca=='bs'||$ca=='ru'||$ca=='sr'||$ca=='uk'?($mf%10==1&&$mf%100!=11?0:($mf%10>1&&$mf%10<5&&$mf/10%10!=1?1:2)):1)))))));$yi=$yi[$jg];}$Fa=func_get_args();array_shift($Fa);$jd=str_replace("%d","%s",$yi);if($jd!=$yi)$Fa[0]=format_number($mf);return
vsprintf($jd,$Fa);}function
switch_lang(){global$ca,$re;echo"<form action='' method='post'>\n<div id='lang'>",lang(19).": ".html_select("lang",$re,$ca,"this.form.submit();")," <input type='submit' value='".lang(20)."' class='hidden'>\n","<input type='hidden' name='token' value='".get_token()."'>\n";echo"</div>\n</form>\n";}if(isset($_POST["lang"])&&verify_token()){cookie("adminer_lang",$_POST["lang"]);$_SESSION["lang"]=$_POST["lang"];$_SESSION["translations"]=array();redirect(remove_from_uri());}$ca="en";if(isset($re[$_COOKIE["adminer_lang"]])){cookie("adminer_lang",$_COOKIE["adminer_lang"]);$ca=$_COOKIE["adminer_lang"];}elseif(isset($re[$_SESSION["lang"]]))$ca=$_SESSION["lang"];else{$va=array();preg_match_all('~([-a-z]+)(;q=([0-9.]+))?~',str_replace("_","-",strtolower($_SERVER["HTTP_ACCEPT_LANGUAGE"])),$Ie,PREG_SET_ORDER);foreach($Ie
as$A)$va[$A[1]]=(isset($A[3])?$A[3]:1);arsort($va);foreach($va
as$y=>$zg){if(isset($re[$y])){$ca=$y;break;}$y=preg_replace('~-.*~','',$y);if(!isset($va[$y])&&isset($re[$y])){$ca=$y;break;}}}$zi=$_SESSION["translations"];if($_SESSION["translations_version"]!=424438045){$zi=array();$_SESSION["translations_version"]=424438045;}function
get_translations($qe){switch($qe){case"en":$g="A9DÂ“yÔ€s:Ã€Gà¡¨Â¸ffÂƒÂ‚ÂŠÂ¦ã‰ˆÙ‚:Ä‡SÂÂ°Ş¡2\"1Â¦..L'ÂƒIÂ´ê­‘#Ç³,Â†KÂƒÂšOP#IÌ€%9ÂÂ¥i4È¯2ÏÆ³Â Â€Ë¬9Â%Ã€PÃ€b2ÂÂ£aÂ¸à²œn2Â›NCÈ¨Ş²4Â™Í±C`(Â:Ebç¹È©:Â‰&ã™”å¹·ÂˆFó½”YÂ‚Âˆ\rÂ´\nÂ– 8ZÔ“=\$AÂÂœÂ†Â¤`Ñ½ËœÂŒÂ²Â‚Â0ÊœnÒ£dFé‰ŒŞ®:ZÎ°)Â­ã‘¦ÕˆmwÛ¸Â€İÂ¼ê­¦pQË—Î‚Â‰Â†qÂœê¡ŠÄ¯Â±#qÂ®Â–w7SÂX3Â”Â Â‰ÂœÂŠÂ˜oÂ¢\n>ZÂ—MÂ„ziÃ„s;ÙŒÂ’Â‚Â„_Å‚:Ã¸Ãµğ£¼€è´¶ÂƒÃºÂ¾\r-z|Â (j*ÂœÂ¨ÂŒ0Â¦:-hæ©¯Ì¸ò¸©«r^1/Ğ›Â¾Î·,ÂºZÓˆKXÂ¹,Â¢pÊº>#ÂÖ…ã¨6Å±CÂŠÂÂ´IÃº|Â³Â©È¢,Â(y Â¸,	%b{ÂµÊ¢Â°Âµóš¹‚ÂƒÂ’)BÂƒß–ÂŒÂ+Ã€1>îÂµ\rÒŠÃ¼6Â¹Â’2ÂšÂLÂ€PÂ‚2\rÂ«\\*ò©Š¢Â=mè‚±ÂŒjHæ¡¤O\$Â¾Ã»Â„Âºğ´ ªjFÃ·oÂƒä–„â†´ #0z\rÂ è¸aĞÂÃµ\\ÂN-Ã»ÂºÂ³ÂŒá¼ŒÑ”pä²…á’€\rÂ«:x7Ë¼Ğ˜Âµá 0Â‡Í£Â2ÂŒjk6Â¸Âµ@îƒ‚Â­ÂŠÂ„Â´âˆ˜ÎA&2Âóµ§œnÂ®1Ãºİ¬Ä +Ä£sÂ‡	Â„Â©Ä¼Â³Â MÒ]lì¦¡Âã¢Ÿ2ÂŠÂÂ»Oz\rÂ±Â£a7Â¬Â¶1Â 7ò²£ŒÃˆë©šÃœrì“ŠvÂŠÃ¨İ¢Ã€Â«äœ³ÂŒèˆÎ£2ÂŒN1Â\0SÂ˜<ÂíŸˆï¿½=Â€PÈ¤Ï­cÂ†å¥­Â°Â¨ÄˆÂ»ÂšÃ¿Í˜ÃŸÂŠaccCÂ ã›˜Ôœn\"`@×ŸÂ®dÂº7Ã“(æ“›VÂ¥nÌ¶Â–Â¬9ä‹ˆÂ…h8ÂkÂÑ¯kË¯K,Â“)Â‰+Z\"ÂÊ³Â°ÂŒÂ»Â†Â‹å”›Â¦\"MFÂ»Â³Â¯Â¨Â¾'iÊŒB\rÂ¿Âƒ0Ì¶NRLÇ„ÂBÂ ŞMpò¯–tÃ»FÂŒØs1Â·tÂ„!Íº\n×°Â»7}KÂ€Â´`O-Ì¤Ã¼â‘¾OİÂ…6tÂ¬Â¼PÂ‘cÃ½_W×¶WÂ’PÂ½ÂØ°ÒÂˆbÂ˜Â¤#2ã¸—#Ã\"Ö²íŸˆï¿½xPİ‰ôŒ¯œtZP*1nÂª}\\Ú¯Í7Ô«Ãµ ×€Â™}Â«I1TÂ­/LÓ´ï¿½CÂºÂ£N	É“Â‡%RÂªÊ¹WÂÂ¹XÂƒè’ˆjÂ¾X	FÂ±Â”IÂŠ@ÂÂ·Â“Æ¯Ë©-dÂ Â•\$ÃµÂªÂ«Rä—ƒ\$ÕªÂ¿Â…(TÂ²Â˜SJqO*DÂ©  .U\nÂª\$TÂÂsÍ†Â°h\$Â‘Ğ FÂ‡Ã¼7Â’0ÂšÂšá½©Â4Â›(ÂŸÍºw=GÂ²%Â’ÂšÂ‡é¢¾nUï¿½Â¡I3BÂŒ`9>Ä¨HCfknyÜº(AÃ»Ù¬ÂÂ…4Â¸4ÂšÂƒK0Æ†EÂ„Ê›ÍºB?Ï¨ï¿½ï¿½Â€H\n-È¢|\n\n\0)\$DÂ‘<Â’LhÂ‰Bï¿½ï¿½Ã½ÂŒ×¢dq2Â¤Â…Â»ã€¹É‰3Â…Â…è·¼Â— kcÃ¹Â°Â!Â°Ã¾Â‚Ã¼O	ñ£§Â¹>ÂºÈ´ëƒƒ2	lÃ¹(S>Éœ"QiS+Ã€Ï&ÂŒ~M)%dÂ´Â—Â›ä¶…Oì±°Â’<Â’ÂœpÈÂŠz}&Â°Â²HcÆšØœ$H\$Â‘AÂ‹Â¹!Â–ÃµìŸ³@@CÂ‰Â³*\$ÂÃÂ“á£Ñ™dxñ¢¼{ÂŒÂ«ç²«Ã€Â´Éœ0ÂÂ£W/T)UÂ5hÒ‚ZÂ³ÃÂ±P×´Â_Â¡%9ÃÉEÂ’î€˜ÃÂ%)Â¥ÂœÃ²GÂ‰ÂTT)Â´Â’HÂ‚ÂŒÂ™#ÃRIÂ´å‚œÂé¸ ÂˆÌƒÂ¥\"Ã¼PÕ±i;/M'\0Â£\\LÂ©je	á¸PÂ TÂ *Â­Â‚\0ÂˆB`EÂ¬MÂzÂ¯s}\r:ZÂ¥Â‘-\nÂ¥sN0ggÂ<Â±Â±Â”&Â›ç‘€%!Â¼Õ‘Ë†jÂŠ9ØTuZÂ˜Ù©-Â]Lï¿½ï¿½,bÍ’HÇÃÂÂ©fÄœ\2Æ²ÍÂ±\"ÂxNÂ˜P0n|ÂŸ9Â¢Â‚Â²'Â(Ã½	Â ó¸¾ˆa>JrDÂ“Ã¾`Ã¨g\rBIIDÂ!Â‘Ò¢cÂ’<ñ›’bálÂ•zUÂˆå•Âµ'Kí™ŒÂ“Â“h!ÂKÂ¡Î˜Ã·